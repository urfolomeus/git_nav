#!/usr/bin/env ruby

$LOAD_PATH.unshift File.expand_path('../../lib', __FILE__)
require 'git_nav'

repo = File.join(File.dirname(__FILE__), '../..', 'css_pal')
git_nav = GitNav::GitNavigator.new(repo)
puts "Commits: #{git_nav.commits.inspect}"
puts "Current commit:\n#{git_nav.log}"
git_nav.next
puts "Current commit:\n#{git_nav.log}"
git_nav.next
puts "Current commit:\n#{git_nav.log}"
git_nav.prev
puts "Current commit:\n#{git_nav.log}"

